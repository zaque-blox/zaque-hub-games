local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Team"
})

Tab:AddButton({
    Name = "Team Blue: GK",
    Callback = function()
        -- Define os argumentos para a função FireServer
        local args = {
            [1] = "Home";
            [2] = "GK";
        }

        -- Dispara o evento remoto no servidor
        game:GetService("ReplicatedStorage")
            :WaitForChild("Packages", 9e9)
            :WaitForChild("Knit", 9e9)
            :WaitForChild("Services", 9e9)
            :WaitForChild("TeamService", 9e9)
            :WaitForChild("RE", 9e9)
            :WaitForChild("Select", 9e9)
            :FireServer(unpack(args))

        -- Mensagem de depuração
        print("Botão pressionado, código executado")
    end    
})

Tab:AddButton({
	Name = "Team White: GK",
	Callback = function()
      	print("button pressed")

